{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        
        "no-console": "off"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "globals": {
        "fixture": true,
        "expect": true,
        "window": true,
        "document": true,
        "$":true,
		"google": true,
		"history": true,
